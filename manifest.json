{
    "manifest_version": 3,
    "name": "Unimelb Hand-bot",
    "description": "A chatbot with data about the University of Melbourne Handbook",
    "version": "1.0",
    "icons": {
        "16": "Chatbot_logo_16.png",
        "48": "Chatbot_logo_48.png",
        "128": "Chatbot_logo_128.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://handbook.unimelb.edu.au/*"],
            "js": ["content.js"],
            "css": ["styles.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["test_popup.html", "Chatbot_logo_128.png", "deepChat.bundle.js", "microsoft.cognitiveservices.speech.sdk.bundle.js", "chatbot_styles.js"],
            "matches": ["https://handbook.unimelb.edu.au/*"]
        }
    ]
}